module data/builtin-types

imports
	include/DiversiaScript


signature constructors
	
	BuiltinType: Name * List(BuiltinType) * List(Constructor) * List(Property) * List(Function) -> BuiltinType
	Property: Name * Type * Writable -> Property
	Function: Name * ReturnType * List(ParamType) -> Function

rules
	
	is-builtin-type(|type) = <get-builtin-type> type
	is-builtin-property(|type,property) = <get-builtin-property> (type,property) 
	is-builtin-function(|type,function) = <get-builtin-function> (type,function)
	is-builtin-plugin(|name) = <fetch(?name)><plugins>
	is-builtin-component(|name) = <fetch(?name)><components>

	get-builtin-type: type -> <fetch-elem(has-name(|type))><builtin-types> 
	get-builtin-type: CustomType(type) -> <get-builtin-type> type
		
	get-builtin-property: (type, property) -> 
		<fetch-elem(has-name(|property))><get-builtin-properties> type
	
	get-builtin-function: (type, function) -> 
		<fetch-elem(has-name(|function))><get-builtin-functions> type

	has-name(|name) =  ?BuiltinType(name,_,_,_,_)
	has-name(|name) =  ?Property(name,_)
	has-name(|name) =  ?Function(name,_,_)

	get-builtin-properties: type -> <get-builtin-properties><get-builtin-type> type	
	get-builtin-properties: BuiltinType(_,parents,_,properties,_) -> 
		<conc>(properties,<mapconcat(get-builtin-type; get-builtin-properties)>parents)
		
	get-builtin-functions: type -> <get-builtin-functions><get-builtin-type> type
	get-builtin-functions: BuiltinType(_,parents,_,_,functions) -> 
		<conc>(functions,<mapconcat(get-builtin-type; get-builtin-functions)>parents)
		
	get-builtin-parents: type -> <get-builtin-parents><get-builtin-type> type
	get-builtin-parents: s@BuiltinType(_,parents,_,_,_) -> 
		<conc>(<map(get-builtin-type)> parents, <mapconcat(get-builtin-type; get-builtin-parents)>parents)
		
