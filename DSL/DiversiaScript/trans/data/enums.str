module data/enums

imports
	
	include/DiversiaScript

signature constructors
	
	Enum: Name * List(EnumValue) -> Enum
	EnumValue: Name * Value -> EnumValue
	
rules
	
	is-enum(|name) = <fetch(has-name(|name))> <enums>

	get-enum: name -> <fetch-elem(has-name(|name))> <enums>
	get-enum-values: name -> values
		where Enum(_, values) := <get-enum> name
	
	get-enum-value: (name, valueName) -> 
		<fetch-elem(has-name(|valueName))> <get-enum-values> name
	get-enum-value-name: (name, value) -> 
		<fetch-elem(has-value(|value))> <get-enum-values> name	
	
	has-name(|name) = ?Enum(name, _)
	has-name(|name) = ?EnumValue(name, _)
	has-value(|value) = ?EnumValue(_, value)
	
	get-name: Enum(name, _) -> name
	get-name: EnumValue(name, _) -> name
	get-value: EnumValue(_, value) -> value
	
